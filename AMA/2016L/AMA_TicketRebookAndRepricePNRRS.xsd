<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://xml.amadeus.com/2010/06/Ticket_RebookAndRepricePNR_v1" xmlns:ama="http://xml.amadeus.com/2010/06/Types_v3" xmlns:far="http://xml.amadeus.com/2010/06/FareTypes_v2" xmlns:far_int="http://xml.amadeus.com/2010/06/FareInternalTypes_v2" xmlns:ota="http://www.opentravel.org/OTA/2003/05/OTA2011A" xmlns:pnr="http://xml.amadeus.com/2010/06/PNR_Types_v4" xmlns:qot="http://xml.amadeus.com/2010/06/QuotationTypes_v2" xmlns:rtl="http://xml.amadeus.com/2010/06/Retailing_Types_v2" targetNamespace="http://xml.amadeus.com/2010/06/Ticket_RebookAndRepricePNR_v1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.000" id="AMA2016L">
  <xs:import namespace="http://www.opentravel.org/OTA/2003/05/OTA2011A" schemaLocation="../../OTA/2011A/OTA_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/Retailing_Types_v2" schemaLocation="AMA_Retailing_CommonTypes_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/FareTypes_v2" schemaLocation="AMA_FareElementCommonTypes_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/QuotationTypes_v2" schemaLocation="../2016H/AMA_QuotationCommonTypes_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/PNR_Types_v4" schemaLocation="../2016H/AMA_PNR_CommonTypes_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/Types_v3" schemaLocation="../2016B/AMA_CommonTypes_Proxy.xsd" />
  <xs:import namespace="http://xml.amadeus.com/2010/06/FareInternalTypes_v2" schemaLocation="AMA_FareInternalCommonTypes_Proxy.xsd" />
  <xs:element name="AMA_TicketRebookAndRepricePNRRS">
    <xs:annotation>
      <xs:documentation>Rebook &amp; Reprice response in stateless or stateful mode</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice>
        <xs:element name="Success">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Warnings" type="rtl:WarningsType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>A successfully processed message, but one in which warnings are generated.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Reservation" type="rtl:ReservationType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Reservation information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Commit" type="rtl:CommitType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Commit information</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Rebooking" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CancelledElements" type="rtl:AssociationsType" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>A list of PNR elements which have been cancelled by RebookAndReprice.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="FrequentFlyers" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Frequent Flyer Cards Information</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="FrequentFlyer" minOccurs="0" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Frequent Flyer Card element</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:annotation>
                                <xs:documentation xml:lang="en">Frequent Flyer Card element in response</xs:documentation>
                              </xs:annotation>
                              <xs:sequence>
                                <xs:element name="FrequentFlyerCard">
                                  <xs:annotation>
                                    <xs:documentation xml:lang="en">Frequent Flyer Card information</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:complexContent>
                                      <xs:extension base="rtl:FrequentFlyerCardType">
                                        <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                      </xs:extension>
                                    </xs:complexContent>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="MileageRequests" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="MileageRequest" minOccurs="1" maxOccurs="99">
                                        <xs:annotation>
                                          <xs:documentation xml:lang="en">miles accrual request</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:annotation>
                                            <xs:documentation xml:lang="en">Frequent Flyer Mileage request element</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexContent>
                                            <xs:extension base="rtl:MileageRequestType">
                                              <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                            </xs:extension>
                                          </xs:complexContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="RedemptionRequests" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="RedemptionRequest" minOccurs="1" maxOccurs="99">
                                        <xs:annotation>
                                          <xs:documentation xml:lang="en">redemption request</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:annotation>
                                            <xs:documentation xml:lang="en">redemption request element</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexContent>
                                            <xs:extension base="rtl:RedemptionRequestType">
                                              <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                            </xs:extension>
                                          </xs:complexContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="UpgradeRequests" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="UpgradeRequest" minOccurs="1" maxOccurs="99">
                                        <xs:annotation>
                                          <xs:documentation xml:lang="en">Upgrade request</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:annotation>
                                            <xs:documentation xml:lang="en">Upgrade request element</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexContent>
                                            <xs:extension base="rtl:UpgradeRequestType">
                                              <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                            </xs:extension>
                                          </xs:complexContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Bounds" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">New air itinerary after the rebooking</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Bound" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation>Bound added to the itinerary of the PNR (bounds can contain open segments).</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Segment" type="rtl:AirSegmentType" minOccurs="0" maxOccurs="99" />
                              </xs:sequence>
                              <xs:attribute name="NIP" type="ama:NumericStringLength1to99" use="optional">
                                <xs:annotation>
                                  <xs:appinfo source="http://xml.amadeus.com/ErrorCodes">
                                    <ama:error code="1194">INVALID NUMBER IN PARTY</ama:error>
                                  </xs:appinfo>
                                  <xs:documentation xml:lang="en">Number in party</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Contacts" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation xml:lang="en">Contacts Information</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Contact" minOccurs="0" maxOccurs="99">
                            <xs:complexType>
                              <xs:annotation>
                                <xs:documentation xml:lang="en">Contact element in response</xs:documentation>
                              </xs:annotation>
                              <xs:complexContent>
                                <xs:extension base="rtl:ContactType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="SpecialServiceRequests" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="SpecialServiceRequest" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Special Service Request information</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="rtl:AncillaryServiceType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Seats" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Seat" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Seat Request information</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="rtl:SeatType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="OtherServicesInformation" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="OtherServiceInformation" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Other Service Information Request</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="rtl:OSIType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="SpecialKeywords" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="SpecialKeyword" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Special Keyword information</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="rtl:SKType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Remarks" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Remark" minOccurs="1" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Remark information</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="rtl:RemarkType">
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="TimeLimits" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="TimeLimit" type="rtl:TimeLimitType" minOccurs="0" maxOccurs="99">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">Time limits created from the request.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="FareDiscounts" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>FD elements which have been created by RebookAndReprice.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="FareDiscount" minOccurs="1" maxOccurs="20">
                            <xs:annotation>
                              <xs:documentation xml:lang="en">A FD element that has been created by this RebookAndReprice transaction.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:complexContent>
                                <xs:extension base="far:DiscountType">
                                  <xs:sequence>
                                    <xs:element name="Associations" type="rtl:AssociationsType" minOccurs="0" />
                                  </xs:sequence>
                                  <xs:attributeGroup ref="rtl:CommonIdentifierAttributes" />
                                </xs:extension>
                              </xs:complexContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Repricing" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Repricing output</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ItineraryRepricing" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>Refer to FareRePriceRS for details</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="PricingDetails">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Totals" type="far_int:TotalsType" minOccurs="0" />
                                <xs:element name="PricingRecords" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="PricingRecord" type="far_int:PricingRecordType" minOccurs="0" maxOccurs="999" />
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Quotations" minOccurs="0">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Quotation" type="far_int:QuotationType" minOccurs="0" maxOccurs="999" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Failure">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Warnings" type="rtl:WarningsType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Message warnings</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="Errors" type="rtl:ErrorsType" minOccurs="0">
                <xs:annotation>
                  <xs:documentation>Message errors</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:attributeGroup ref="ota:OTA_PayloadStdAttributes" />
    </xs:complexType>
  </xs:element>
  <xs:complexType name="ItineraryPriceType">
    <xs:sequence>
      <xs:element name="Totals" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Price" minOccurs="0" maxOccurs="99">
              <xs:complexType>
                <xs:attributeGroup ref="qot:PriceAttributeGroup" />
              </xs:complexType>
            </xs:element>
            <xs:element name="Points" type="qot:PointType" minOccurs="0" maxOccurs="99" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="PricingRecords" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="PricingRecord" minOccurs="0" maxOccurs="999">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="PaxName" minOccurs="0">
                    <xs:complexType>
                      <xs:attribute name="Title" type="xs:string" />
                      <xs:attribute name="LastName" type="xs:string" />
                      <xs:attribute name="FirstName" type="xs:string" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Price" minOccurs="0" maxOccurs="99">
                    <xs:complexType>
                      <xs:attributeGroup ref="qot:PriceAttributeGroup" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Tax" minOccurs="0" maxOccurs="999">
                    <xs:complexType>
                      <xs:attributeGroup ref="qot:TaxAttributeGroup" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Fee" minOccurs="0" maxOccurs="99">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="Price" minOccurs="0" maxOccurs="99">
                          <xs:complexType>
                            <xs:attributeGroup ref="qot:PriceAttributeGroup" />
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="Tax" minOccurs="0" maxOccurs="99">
                          <xs:complexType>
                            <xs:attributeGroup ref="qot:TaxAttributeGroup" />
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                      <xs:attribute name="Type" use="optional">
                        <xs:annotation>
                          <xs:documentation>Fee Type (example: OB)</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="10" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute name="Code" use="optional">
                        <xs:annotation>
                          <xs:documentation>Fee code (axample: FC1 or T01)</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="5" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute name="Description" use="optional">
                        <xs:annotation>
                          <xs:documentation>Fee commercial description</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="250" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Points" minOccurs="0" maxOccurs="99">
                    <xs:complexType>
                      <xs:complexContent>
                        <xs:extension base="qot:PointType" />
                      </xs:complexContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="PricingInfo" minOccurs="0">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="AssociatedPNRElement" minOccurs="0" maxOccurs="99">
                          <xs:complexType>
                            <xs:attribute name="Type" type="ama:AlphaNumericStringLength1to10" use="optional">
                              <xs:annotation>
                                <xs:documentation>Passenger / Segment / etc...</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="Tattoo" type="xs:NMTOKEN" use="optional">
                              <xs:annotation>
                                <xs:documentation>Tattoo of the element</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="RefIDs" type="xs:NMTOKEN" use="optional">
                              <xs:annotation>
                                <xs:documentation source="Simple space separated list of UUIDs that this element is linked to." />
                              </xs:annotation>
                            </xs:attribute>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="ServiceProvider" minOccurs="0" maxOccurs="10">
                          <xs:complexType>
                            <xs:simpleContent>
                              <xs:extension base="qot:CarrierCodeType">
                                <xs:attribute name="Type">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:maxLength value="30" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:attribute>
                              </xs:extension>
                            </xs:simpleContent>
                          </xs:complexType>
                        </xs:element>
                        <xs:element name="OtherInfo" minOccurs="0" maxOccurs="99">
                          <xs:complexType>
                            <xs:simpleContent>
                              <xs:extension base="xs:string">
                                <xs:attribute name="Name" type="ama:AlphaNumericStringLength1to20" />
                              </xs:extension>
                            </xs:simpleContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                      <xs:attribute name="PricingRecordRefIDs" type="xs:NMTOKEN" use="optional">
                        <xs:annotation>
                          <xs:documentation>Tattoo of the created TST</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="CreationDate" type="xs:date" use="optional">
                        <xs:annotation>
                          <xs:documentation xml:lang="en">Creation Date of the TST</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="PassengerTypeCode" type="ota:AlphaNumericStringLength1to8" use="optional">
                        <xs:annotation>
                          <xs:documentation xml:lang="en">Code used to indicate the type of traveler that will be traveling (e.g., ADT, CHD, INF, GRP, NCG, COG ; the two last ones means Non Commercial Group and Corporate Group).</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="Endorsement" use="optional">
                        <xs:annotation>
                          <xs:documentation>Free-flow text Fare Calculation</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="250" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute name="FareCalc" use="optional">
                        <xs:annotation>
                          <xs:documentation>Free-flow text Fare Calculation</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="160" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute name="MileageCalc" use="optional">
                        <xs:annotation>
                          <xs:documentation>Mileage text Fare Calculation</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                          <xs:restriction base="xs:string">
                            <xs:maxLength value="160" />
                          </xs:restriction>
                        </xs:simpleType>
                      </xs:attribute>
                      <xs:attribute name="PricingRecordIndicator" type="ama:AlphaNumericStringLength1to10" />
                      <xs:attribute name="PricingType" type="ama:AlphaNumericStringLength1to10">
                        <xs:annotation>
                          <xs:documentation>Used to distinguish TST, TSM and TQR data</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                      <xs:attribute name="RFIC" type="ama:AlphaNumericStringLength1to10" />
                      <xs:attribute name="RFISC" type="ama:AlphaNumericStringLength1to10" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="FareComponent" minOccurs="0" maxOccurs="99">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="AssociatedPNRElement" minOccurs="0" maxOccurs="99">
                          <xs:complexType>
                            <xs:attribute name="Type" type="ama:AlphaNumericStringLength1to10" use="optional">
                              <xs:annotation>
                                <xs:documentation>Passenger / Segment / etc...</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="Tattoo" type="xs:NMTOKEN" use="optional">
                              <xs:annotation>
                                <xs:documentation>Tattoo of the element</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="RefIDs" type="xs:NMTOKEN" use="optional">
                              <xs:annotation>
