                    <xs:documentation xml:lang="en">Name of the departure city</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="airportName" type="ama:StringLength1to60orString0" use="optional">
                  <xs:annotation>
                    <xs:documentation>Name of the departure airport</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="cityCode" type="ama:StringLength1to60orString0" use="optional">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">code of the departure city</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="countryCode" type="ama:StringLength1to60orString0" use="optional">
