                    <xs:documentation xml:lang="en">Name of the arrival city</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="airportName" type="ama:StringLength1to60orString0" use="optional">
                  <xs:annotation>
