                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="cityCode" type="ama:StringLength1to60orString0" use="optional">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">code of the arrival city</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
                <xs:attribute name="countryCode" type="ama:StringLength1to60orString0" use="optional">
                  <xs:annotation>
                    <xs:documentation xml:lang="en">code of the arrival country</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
