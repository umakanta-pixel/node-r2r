  </xs:simpleType>
  <xs:simpleType name="AlphaNumericString_Length1To14">
    <xs:annotation>
      <xs:documentation xml:lang="en">Used for Alpha-Numeric Strings, length 1 to 14.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
