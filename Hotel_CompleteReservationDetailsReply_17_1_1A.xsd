            <xs:element name="textOptions" minOccurs="0" maxOccurs="2" type="MiscellaneousRemarksType" />
            <xs:element name="taxSection" minOccurs="0" maxOccurs="200">
              <xs:annotation>
                <xs:documentation xml:lang="en">This group is used to convey the detail of each tax, service, surcharge belonging to: - 'rate inclusion / extra' category or - 'all knwon taxes' category  The specifications require the possibility to convey up 200 of them.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="taxFeeInformation" type="HotelTaxesServiceChargesFeesType">
                    <xs:annotation>
                      <xs:documentation xml:lang="en">This segment is used to convey the tax, fee, service, surcharge or extra information.</xs:documentation>
