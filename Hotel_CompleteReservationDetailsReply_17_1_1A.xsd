  <xs:element name="Hotel_CompleteReservationDetailsReply">
    <xs:annotation>
      <xs:documentation xml:lang="en">Hotel Complete Reservations Details Response
Hotel Sales Requirements Query response
This message contains all the hotel sales requirement information returned either by the provider/CRS/switch or the database. H for Hotel SLR for Sales Requirements R for Read R for Reply</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="errorInformation" minOccurs="0" type="ErrorGroupType">
          <xs:annotation>
            <xs:documentation xml:lang="en">This group is returned in case of error</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="retrievalKeyGroup">
          <xs:annotation>
            <xs:documentation xml:lang="en">this group regroups the rloc and the tattoo.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="tattooNumber" type="ElementManagementSegmentType">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment conveys the element tattoo : - Segment tattoo for an Hotel booking - Offer tattoo for an Hotel Offer</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element name="retrievalKey" minOccurs="0" type="ReservationControlInformationTypeI">
                <xs:annotation>
                  <xs:documentation xml:lang="en">This segment is used to convey the record locator of the PNR if available.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="hotelSalesRequirementsSection" minOccurs="0" type="HotelReservationPropertiesType">
          <xs:annotation>
            <xs:documentation xml:lang="en">This group is used to convey the information returned on a normal response of the Hotel sales requirements (when no error occured).</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
