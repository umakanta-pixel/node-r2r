      <xs:element name="TrainNumber" type="TrainNumberType">
        <xs:annotation>
          <xs:documentation xml:lang="en">The unique number that identifies the train.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NetworkCode" type="NetworkCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The network code of a rail operator defined by UIC (International Union of Railways.)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TrainInfoType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defines train Information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Train" type="TrainIdentificationType">
