      <xs:element name="HotelMeetingFacility" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en">Identifies hotel meeting facility information for a property.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="MeetingRoomCount" type="xs:long" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">The total number of unique meeting rooms provided at the hotel facility. For example, if the hotel has a grand ballroom that breaks into Salon A, B, and C -the total number of unique meeting rooms is three.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <!--Replaced incorrect type "xs:nonNegativeInteger"-->
          <xs:attribute name="LargestSeatingCapacity" type="xs:long" use="optional">
            <xs:annotation>
