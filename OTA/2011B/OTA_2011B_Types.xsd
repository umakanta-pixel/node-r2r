          <xs:attributeGroup ref="DatePeriodGroup" />
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="DelayTime" use="optional">
      <xs:annotation>
        <xs:documentation>Delayed time in seconds.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ScheduleCode" type="OTA_CodeType" use="optional">
      <xs:annotation>
        <xs:documentation>The schedule code for the train. Refer to OpenTravel code list Train Schedule Code (TSC).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="TrainQueryType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defines information to do a query by train operator or specific train information.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="Train" type="TrainIdentificationType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">A specific train and network code.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NetworkCode" type="NetworkCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The network code of a rail operator defined by International Union of Railways (UIC.)</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="TrainSegmentSummaryType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defines train segment summary information, including departure and arrival stations, departure and arrival dates/times, and train identification information.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DepartureStation" type="StationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Train departure station information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ArrivalStation" type="StationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Train arrival station information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DepartureDateTime" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="en">The departure date and time.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ArrivalDateTime" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="en">The arrival date and time.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TrainIdentification" type="TrainIdentificationType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Train identification information, including  a train number and a train network code.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="TrainSegmentType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Defines information about a train segment.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DepartureStation" type="StationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">The departure point of the train segment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ArrivalStation" type="StationDetailsType">
        <xs:annotation>
          <xs:documentation xml:lang="en">The arrival point of the train segment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="MarketingCompany" type="CompanyNameType">
        <xs:annotation>
          <xs:documentation xml:lang="en">The marketing train company.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OperatingCompany" type="CompanyNameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The operating train company, if different from the Marketing Company.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Equipment" type="ClassCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">The type of equipment used for the train journey. Use a string value, such as the operator equipment code or a UIC code.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TrainInfo" type="TrainInfoType" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en">Detailed information about the train.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="DepartureDateTime" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">The departure date and time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="ArrivalDateTime" type="xs:dateTime" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">The arrival date and time.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <!--Replaced incorrect type "xs:nonNegativeInteger"-->
    <xs:attribute name="StopQuantity" type="xs:long" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">The number of stops the train makes.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="JourneyDuration" type="xs:duration" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">The duration of the train from departure location to destination location.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="CrossBorderInd" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">When true, the train schedule includes a border crossing.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="TravelPurposeType">
    <xs:annotation>
      <xs:documentation xml:lang="en">Rail Passenger travel purpose with an associated attribute to capture additional enumerated values as agreed upon between trading partners.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="TravelPurposeEnum">
        <xs:attribute name="extension" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation xml:lang="en">A string value that represents an "Other_", or extended, enumerated value agreed upon between trading partners.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation xml:lang="en">All Schema files in the OpenTravel Alliance specification are made available according to the terms defined by the OpenTravel License Agreement at http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>
  </xs:annotation>
  <xs:attributeGroup name="TrainPrefGroup">
    <xs:annotation>
      <xs:documentation source="Use" xml:lang="en">Indicates preferences for certain types of trains, such as connections or stopovers, when used for a specific travel situation.</xs:documentation>
    </xs:annotation>
    <!--Replaced nested attributeGroup {http://www.opentravel.org/OTA/2003/05}PreferLevelGroup by content from OTA_2011B_Types-->
    <xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Used to indicate a level of preference for an associated item.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="TrainType" type="OTA_CodeType" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Refer to OpenTravel Code List Train Type Code (TTC).</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <!--Replaced incorrect type "xs:nonNegativeInteger"-->
    <xs:attribute name="MaxConnections" type="xs:long" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Indicates that if connection is chosen, then this attribute defines the maximum number of connections preferred.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="DirectAndNonStopOnlyInd" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">If true, direct and non-stop flights are requested.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="NonStopsOnlyInd" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">If true, only non-stop flights are requested.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="RoutingType" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Specifies the travel routing preference.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction base="xs:NMTOKEN">
          <xs:enumeration value="Normal">
            <xs:annotation>
              <xs:documentation xml:lang="en">No special conditions.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
          <xs:enumeration value="Mirror">
            <xs:annotation>
              <xs:documentation xml:lang="en">Outbound and inbound route of travel must be the same.</xs:documentation>
            </xs:annotation>
          </xs:enumeration>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="SameOperatorOnlyInd" type="xs:boolean" use="optional">
      <xs:annotation>
        <xs:documentation>Same carrier/operator only.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="CodeGroupPref">
    <xs:annotation>
      <xs:documentation source="Use" xml:lang="en">Specifies preference level for the item identified by the code.</xs:documentation>
    </xs:annotation>
    <!--Replaced nested attributeGroup {http://www.opentravel.org/OTA/2003/05}CodeGroup by content from OTA_2011B_Types-->
    <xs:attribute name="Code" type="StringLength1to8" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Any code used to specify an item, for example, type of traveler, service code, room amenity, etc.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="CodeContext" type="StringLength1to32" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Identifies the source authority for the code.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="PreferLevel" type="PreferLevelType" use="optional">
      <xs:annotation>
        <xs:documentation xml:lang="en">Used to indicate a level of preference for an associated item.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:complexType name="LocationPrefType">
    <xs:annotation>
      <xs:documentation source="Use" xml:lang="en">Location preference.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="LocationType">
        <xs:attributeGroup ref="PreferLevelGroup" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
