          <xs:documentation xml:lang="en">Identification and classification information of the train.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValidDate" minOccurs="0">
        <xs:annotation>
          <xs:documentation>The period for which this train information is valid.</xs:documentation>
        </xs:annotation>
