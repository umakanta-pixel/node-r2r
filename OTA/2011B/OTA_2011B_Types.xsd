        <xs:complexType>
          <xs:attribute name="SecurityFeatureCode" type="OTA_CodeType" use="optional">
            <xs:annotation>
              <xs:documentation xml:lang="en">Identifies a security feature at a propetry. Refer to OpenTravel Code list Security Feature Code (SEC).</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="AccessibilityCode" type="OTA_CodeType" use="optional">
