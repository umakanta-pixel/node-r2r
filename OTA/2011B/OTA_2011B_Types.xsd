              <xs:documentation xml:lang="en">A fare basis code.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:complexType>
      </xs:element>
      <xs:element name="TrainPref" minOccurs="0" maxOccurs="9">
